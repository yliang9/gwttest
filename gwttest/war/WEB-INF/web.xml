<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Gwttest.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>configuratorServiceImpl</servlet-name>
    <servlet-class>org.mc.client.server.ConfiguratorServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>configuratorServiceImpl</servlet-name>
    <url-pattern>/gwttest/configurator</url-pattern>
  </servlet-mapping>
  
  <listener>
  	<listener-class>
  		org.biomart.configurator.controller.BiomartConfig
  	</listener-class>
  </listener>

</web-app>
